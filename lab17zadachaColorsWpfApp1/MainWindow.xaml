<Window x:Class="lab17zadachaColorsWpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab17zadachaColorsWpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="600">
    <StackPanel Orientation="Vertical" VerticalAlignment="Center">
        <local:ColorPicker x:Name="colorpick" Margin="10" ColorChanged="colorpick_ColorChanged"/>
        <TextBox x:Name="tbx" Text="Color definition" Margin="10" HorizontalAlignment="Right"/>
    </StackPanel>
</Window>
